[{"type":"js","data":"(function(){// -- Router Config --\nRouter.configure({\n  layoutTemplate: 'appLayout',\n  loadingTemplate: 'loading',\n  progressSpinner : false,\n  onAfterAction: function() {\n    document.title = Meteor.settings.public.PROJECT_NAME;\n  }\n});\n\nRouter.plugin('loading', {loadingTemplate: 'loading'});\nRouter.plugin('dataNotFound', {dataNotFoundTemplate: 'notFound'});\n\n\n// Accounts Config\nAccountsTemplates.configureRoute('signIn', {layoutTemplate: 'appLayout'});\nAccountsTemplates.configureRoute('signUp', {layoutTemplate: 'appLayout'});\nAccountsTemplates.addField({\n    _id: 'name',\n    type: 'text',\n    placeholder: {\n        signUp: \"Name\"\n    },\n    required: true,\n    minLength: 4,\n    //re: /(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{6,}/,\n    errStr: 'At least four characters',\n});\n\n}).call(this);\n","servePath":"/both/config/config.js","sourceMap":{"version":3,"sources":["meteor://ðŸ’»app/both/config/config.js"],"names":[],"mappings":"yBAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"/both/config/config.js","sourcesContent":["// -- Router Config --\nRouter.configure({\n  layoutTemplate: 'appLayout',\n  loadingTemplate: 'loading',\n  progressSpinner : false,\n  onAfterAction: function() {\n    document.title = Meteor.settings.public.PROJECT_NAME;\n  }\n});\n\nRouter.plugin('loading', {loadingTemplate: 'loading'});\nRouter.plugin('dataNotFound', {dataNotFoundTemplate: 'notFound'});\n\n\n// Accounts Config\nAccountsTemplates.configureRoute('signIn', {layoutTemplate: 'appLayout'});\nAccountsTemplates.configureRoute('signUp', {layoutTemplate: 'appLayout'});\nAccountsTemplates.addField({\n    _id: 'name',\n    type: 'text',\n    placeholder: {\n        signUp: \"Name\"\n    },\n    required: true,\n    minLength: 4,\n    //re: /(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{6,}/,\n    errStr: 'At least four characters',\n});\n"]}},{"type":"js","data":"(function(){// Home Route\nRouter.route('/', {\n  name: 'home',\n  title: \"Home\"\n});\n\n// Dashboard route\nRouter.route('/dashboard', {\n  name: 'dashboard',\n  title: \"Dashboard\",\n  waitOn: function() {\n    return this.subscribe('meetings');\n  },\n  data: {\n    meetings: Meetings.find()\n\n  },\n  onBeforeAction: function (pause) {\n    AccountsTemplates.ensureSignedIn.call(this, pause);\n  },\n  onAfterAction: function () {\n\n  }\n});\n\n// Profile Route\nRouter.route('/profile', {\n  name: 'profile',\n  title: function() {\n    var user = Meteor.user();\n    var username = user && user.profile && user.profile.name || \"Unknown\";\n    return \"Profile - \" + username;\n  }\n});\n\n\nRouter.onBeforeAction(function() {\n  GoogleMaps.load();\n  this.next();\n}, { only: ['dashboard'] });\n}).call(this);\n","servePath":"/both/router/routes.js","sourceMap":{"version":3,"sources":["meteor://ðŸ’»app/both/router/routes.js"],"names":[],"mappings":"yBAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,4B","file":"/both/router/routes.js","sourcesContent":["// Home Route\nRouter.route('/', {\n  name: 'home',\n  title: \"Home\"\n});\n\n// Dashboard route\nRouter.route('/dashboard', {\n  name: 'dashboard',\n  title: \"Dashboard\",\n  waitOn: function() {\n    return this.subscribe('meetings');\n  },\n  data: {\n    meetings: Meetings.find()\n\n  },\n  onBeforeAction: function (pause) {\n    AccountsTemplates.ensureSignedIn.call(this, pause);\n  },\n  onAfterAction: function () {\n\n  }\n});\n\n// Profile Route\nRouter.route('/profile', {\n  name: 'profile',\n  title: function() {\n    var user = Meteor.user();\n    var username = user && user.profile && user.profile.name || \"Unknown\";\n    return \"Profile - \" + username;\n  }\n});\n\n\nRouter.onBeforeAction(function() {\n  GoogleMaps.load();\n  this.next();\n}, { only: ['dashboard'] });"]}},{"type":"js","data":"(function(){Meteor.methods({\n  createNewUser: (data) => {\n\n    if(data.name.length<3) {\n      throw new Meteor.Error(400, \"Error: Your nickname must be at least 3 characters.\");\n    } else if(data.password.length<8) {\n      throw new Meteor.Error(400, \"Your password must be at least 8 characters.\");\n    } else {\n      return Accounts.createUser({\n        email: data.email,\n        password: data.password,\n        profile: {\n          name: data.name\n        }\n      });\n    } \n  }\n});\n}).call(this);\n","servePath":"/server/methods/user.js","sourceMap":{"version":3,"sources":["/server/methods/user.js"],"names":[],"mappings":"yBAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,G","file":"/server/methods/user.js","sourcesContent":["Meteor.methods({\n  createNewUser: (data) => {\n\n    if(data.name.length<3) {\n      throw new Meteor.Error(400, \"Error: Your nickname must be at least 3 characters.\");\n    } else if(data.password.length<8) {\n      throw new Meteor.Error(400, \"Your password must be at least 8 characters.\");\n    } else {\n      return Accounts.createUser({\n        email: data.email,\n        password: data.password,\n        profile: {\n          name: data.name\n        }\n      });\n    } \n  }\n});"]}}]